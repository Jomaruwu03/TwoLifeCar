{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "env": {
    "MONGODB_URI": "@mongodb_uri",
    "JWT_SECRET": "@jwt_secret",
    "SLACK_WEBHOOK_URL":"@slack_webhook_url",
    "RECAPTCHA_SECRET_KEY": "@recaptcha_secret_key"
  }
}